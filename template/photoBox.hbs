    <div id="photoBox" class="photoBox hidden">
      <div class="pb-close"><a href="#">â•³</a></div>
      <div class="pb-viewer">
        <div class="pb-layer" >
          <div class="pb-main-image loading">
            <img src="" style="display:none" onload="var w=this.width;var h=this.height;$(this).parent().css({width:w, height:h, marginLeft:-w/2, marginTop:-h/2, top:'50%', left:'50%'})">
          </div>
        </div>
        <div class="pb-desc">
          <div class="pb-title">this is desc</div>
          <div class="pb-comments">
            this is comments
          </div>
        </div>
      </div>
      <div class="pb-preview">
        <div class="pb-spliter"></div>
        <div class="pb-left-handler"><a href="#">previus</a></div>
        <div class="pb-right-handler"><a href="#">next</a></div>
        <div class="pb-list-wrapper">
          <ul class="pb-list">
            {{#each list}}
            <li style="background-image:url({{this.thumb}})"><a href="#">title</a></li>
            {{/each}}
          </ul>
        </div>
      </div>
    </div>
    <div id="photoBoxShadow" class="photoBox hidden"/>
